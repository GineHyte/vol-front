{
	"appId": "com.vol-techs.app",
	"productName": "Technical Skills in Beach Volleyball",
	"directories": {
		"output": "dist/artifacts/local",
		"buildResources": "installer/resources"
	},
	"files": [
		"out"
	],
	"extraFiles": [
		{
			"from": "build/Release",
			"to": "TEST",
			"filter": "*.node"
		}
	],
	"win": {
		"target": "nsis"
	},
	"nsis": {
		"deleteAppDataOnUninstall": true,
		"include": "installer/win/nsis-installer.nsh"
	}
}